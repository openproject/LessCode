            android:name=".activity.FileActivity"
            android:label="@string/title_activity_file" />
        <activity
            android:name=".activity.HttpActivity"
            android:label="@string/title_activity_http" />
        <activity
            android:name=".activity.KeyboradActivity"
            android:label="@string/title_activity_keyborad" />
        <activity
            android:name=".activity.NetworkActivity"
            android:label="@string/title_activity_network" />
        <activity
            android:name=".activity.ResourceActivity"
            android:label="@string/title_activity_resource" />
        <activity
            android:name=".activity.SharedPreferenceActivity"
            android:label="@string/title_activity_shared_preference" />
        <activity
            android:name=".activity.ToastActivity"
            android:label="@string/title_activity_toast" />
        <activity
            android:name=".activity.ViewActivity"
            android:label="@string/title_activity_view" />
        <activity
            android:name=".activity.AppActivity"
            android:label="@string/title_activity_app" />
        <activity
            android:name=".activity.DrawableActivity"
            android:label="@string/title_activity_drawable" />
