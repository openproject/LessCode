<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.jayfeng.lesscode.app">

    <application
        android:name=".MyApp"
        android:allowBackup="true"
        android:icon="@drawable/less_app_update_icon"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
        tools:replace="android:label">
        <activity
            android:name=".activity.MainActivity"
            android:label="@string/title_activity_main">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.LogActivity"
            android:label="@string/title_activity_log" />
        <activity
            android:name=".activity.StorageActivity"
            android:label="@string/title_activity_storage" />
        <activity
            android:name=".activity.AdapterActivity"
            android:label="@string/title_activity_adapter" />
        <activity
            android:name=".activity.adapterless.BaseAdapterActivity"
            android:label="@string/title_activity_adapter_base" />
        <activity
            android:name=".activity.adapterless.PagerAdapterActivity"
            android:label="@string/title_activity_adapter_pager" />
        <activity
            android:name=".activity.UpdateActivity"
            android:label="@string/title_activity_update" />
        <activity
            android:name=".activity.ActivityActivity"
            android:label="@string/title_activity_" />
        <activity
            android:name=".activity.CacheActivity"
            android:label="@string/title_activity_cache" />
        <activity
            android:name=".activity.DeviceActivity"
            android:label="@string/title_activity_device" />
        <activity
            android:name=".activity.DisplayActivity"
            android:label="@string/title_activity_display" />
